{
  "name": "@pearlbea/better-img",
  "version": "1.7.9",
  "description": "A vanilla web component that adds a fallback url and logging function to an image.",
  "main": "src/better-img.js",
  "unpkg": "src/better-img.js",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "2.8.0",
    "chai": "6.0.1",
    "http-server": "14.1.1",
    "karma": "6.4.4",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-firefox-launcher": "2.1.3",
    "karma-mocha": "2.0.1",
    "mocha": "11.7.2",
    "uglify-es": "3.3.10"
  },
  "scripts": {
    "prepare": "npm run minify",
    "start": "http-server",
